{
  "name": "@pyrogenic/toed",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@pyrogenic/asset": "^1.1.0",
    "@pyrogenic/perl": "^1.0.0",
    "lodash": "^4.17.19",
    "mobx": "^5.14.2",
    "mobx-react": "6.1.4",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "open-iconic": "^1.1.1",
    "react": "*",
    "react-bootstrap": "*",
    "react-dom": "*",
    "react-scripts": "*",
    "serializr": "^1.5.3"
  },
  "scripts": {
    "bridge": "node src/bridge.js",
    "pre-webdis": "mkdir -p log && touch log/webdis-cache.log && touch log/webdis-gwf.log",
    "webdis-cache": "submodules/webdis/webdis webdis-cache.json",
    "tail-webdis": "tail -f log/webdis-*.log",
    "start": "npm-run-all --print-label --sequential pre-webdis --parallel start-ts bridge tail-webdis webdis-cache",
    "start-ts": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.144",
    "@types/node": "~14",
    "@types/react": "*",
    "@types/react-dom": "*",
    "typescript": "~4.3.2",
    "eslint": "~7.27.0"
  }
}
