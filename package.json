{
  "name": "toed",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "lodash": "^4.17.15",
    "mobx": "^5.14.2",
    "mobx-react": "6.1.4",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "open-iconic": "^1.1.1",
    "react": "~16.10.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "~16.10.2",
    "react-scripts": "^3.2.0",
    "serializr": "^1.5.3"
  },
  "scripts": {
    "pre-bridge": "gulp build-srv",
    "bridge-srv": "node srv/srv/DictionaryService.js",
    "bridge": "npm-run-all --sequential pre-bridge bridge-srv",
    "pre-webdis": "mkdir -p log && > log/webdis-cache.log && > log/webdis-gwf.log",
    "webdis-cache-srv": "submodules/webdis/webdis webdis-cache.json",
    "webdis-gwf-srv": "submodules/webdis/webdis webdis-gwf.json",
    "webdis-cache-tail": "tail -f log/webdis-cache.log",
    "webdis-gwf-tail": "tail -f log/webdis-gwf.log",
    "webdis-cache": "npm-run-all --parallel webdis-cache-srv webdis-cache-tail",
    "webdis-gwf": "npm-run-all --parallel webdis-gwf-srv webdis-gwf-tail",
    "start": "npm-run-all --print-label --sequential pre-webdis --parallel client bridge webdis-cache webdis-gwf",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chai": "~4.2.7",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.144",
    "@types/gulp": "~4.0.6",
    "@types/mocha": "~5.2.7",
    "@types/node": "12.11.2",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "~16.9.9",
    "@types/react-dom": "~16.9.2",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "mocha": "~7.0.0",
    "gulp": "~4.0.2",
    "gulp-typescript": "~6.0.0-alpha.1",
    "tslint": "^5.20.0",
    "ts-node": "~8.6.2",
    "typescript": "~3.7.3"
  }
}
